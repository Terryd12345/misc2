      // Custom JS & jQuery here


      // Init sidenav
      $('.button-collapse').sideNav();


      // Init carousel
      $('.carousel').carousel();

      // Init Scrollspy
      $('.scrollspy').scrollSpy();


      // Init slider
      $('.slider').slider({
        indicators: false,
        interval: 5000,
        transition: 1000,
        height: 700
